// max function assigned as a "value"
i32 max1 = fn(a: i32, b: i32) -> i32 {
  i32 res = if (a > b) { a } else { b };
  return res;
};

// max function assigned using "def"
// "def" is "syntactic sugar" as they say
def max2 = fn(a: i32, b: i32) -> i32 {
  i32 res = if (a > b) { a } else { b };
  return res;
};

// but they do the same thing

print(max1(1, 10));
print(" = ");
print(max2(1, 10));
print(": ");
println(max1(1, 10)==max2(1, 10));
