// max function assigned as a "value"
i32 max = fn(a: i32, b: i32) -> i32 {
  i32 res = if (a > b) { a } else { b };
  return res;
};

// max function assigned using "def"
// "def" is "syntactic sugar" as they say
def max = fn(a: i32, b: i32) -> i32 {
  i32 res = if (a > b) { a } else { b };
  return res;
};
